	baz.bar[3][4]{"asdf"} *= 2;
	puts(baz.bar[3][4]{"asdf"}--);

        // change original value while processing first group of indices
        baz.bar[0][0]{"foo"} = 1;
        baz.bar[0][(baz.bar[0][0]{"foo"} = 0)]{"foo"} += 1;

        // change original value while processing nested group of indices
        baz.bar[0][1]{"foo"} = "oof";
