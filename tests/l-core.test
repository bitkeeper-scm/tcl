} -output "4 5 6\nfoo 4 bar 5 baz 6\n1024 768\n"

test decl-1.5 {test one-dimensional partial array initializers} -body {
#lang L
void
decl_1_5()
{
	/*
	 * Check that partial array initializers get padded out with
	 * 0/0.0/"" (based on the type).
	 */

	int	vi;
	string	vs;
	float	vf;
	int	a1[];
	int	a2[]  = {0, 1, 2};
	int	a3[3] = {0, 1, 2};
	int	a4[9] = {0, 1, 2};
	float	f1[];
	float	f2[]  = {0.1, 1.1, 2.1};
	float	f3[3] = {0.1, 1.1, 2.1};
	float	f4[9] = {0.1, 1.1, 2.1};
	string	s1[];
	string	s2[]  = {"zero", "one", "two"};
	string	s3[3] = {"zero", "one", "two"};
	string	s4[9] = {"zero", "one", "two"};

	foreach (vi in a1) printf("a1 has %d\n", vi);
	foreach (vi in a2) printf("a2 has %d\n", vi);
	foreach (vi in a3) printf("a3 has %d\n", vi);
	foreach (vi in a4) printf("a4 has %d\n", vi);
	if (defined(a1[3])) puts("bad 1.1");
	if (defined(a2[3])) puts("bad 1.2");
	if (defined(a3[3])) puts("bad 1.3");
	if (defined(a4[9])) puts("bad 1.4");

	foreach (vf in f1) printf("f1 has %3.1f\n", vf);
	foreach (vf in f2) printf("f2 has %3.1f\n", vf);
	foreach (vf in f3) printf("f3 has %3.1f\n", vf);
	foreach (vf in f4) printf("f4 has %3.1f\n", vf);
	if (defined(f1[3])) puts("bad 2.1");
	if (defined(f2[3])) puts("bad 2.2");
	if (defined(f3[3])) puts("bad 2.3");
	if (defined(f4[9])) puts("bad 2.4");

	foreach (vs in s1) printf("s1 has '%s'\n", vs);
	foreach (vs in s2) printf("s2 has '%s'\n", vs);
	foreach (vs in s3) printf("s3 has '%s'\n", vs);
	foreach (vs in s4) printf("s4 has '%s'\n", vs);
	if (defined(s1[3])) puts("bad 3.1");
	if (defined(s2[3])) puts("bad 3.2");
	if (defined(s3[3])) puts("bad 3.3");
	if (defined(s4[9])) puts("bad 3.4");
}
#lang tcl
decl_1_5
} -output "a2 has 0
a2 has 1
a2 has 2
a3 has 0
a3 has 1
a3 has 2
a4 has 0
a4 has 1
a4 has 2
a4 has 0
a4 has 0
a4 has 0
a4 has 0
a4 has 0
a4 has 0
f2 has 0.1
f2 has 1.1
f2 has 2.1
f3 has 0.1
f3 has 1.1
f3 has 2.1
f4 has 0.1
