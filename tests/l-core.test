test loop-1.3 {continue statements} -body {
#lang L
loop_1_3() {
  loop_1_3_for_continue();
  loop_1_3_foreach_continue();
}

loop_1_3_for_continue() {
  int i;

  for (i=0; i<5; i++) {
    if (i == 2) {
      continue;
    }
    puts(i);
  }
}

loop_1_3_foreach_continue() {
  hash h;
  string k, v, l;

  h{"a"} = "x";
  h{"b"} = "y";
  h{"c"} = "z";

  printf("foreach test 1\n");
  foreach (k => v in h) {
    if (k == "b") {
      continue;
    }
    printf("%s => %s\n", k, v);
  }

  printf("foreach test 2\n");
  foreach (l in h) {
    if (l == "c") continue;
    foreach (k => v in h) {
      if (k == "b") {
        continue;
      }
      printf("%s: %s => %s\n", l, k, v);
    }
  }
}
#lang tcl
loop_1_3
} -output {0
1
3
4
foreach test 1
a => x
c => z
foreach test 2
a: a => x
a: c => z
b: a => x
b: c => z
}

